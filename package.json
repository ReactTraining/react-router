{
  "name": "react-router-packages",
  "private": true,
  "scripts": {
    "build": "node ./scripts/build.js",
    "clean": "git clean -fdX .",
    "postinstall": "node ./scripts/postinstall.js && opencollective-postinstall",
    "start": "node ./scripts/start.js",
    "test": "node ./scripts/test.js"
  },
  "devDependencies": {
    "husky": "^1.1.3",
    "lerna": "^3.13.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.4.1"
  },
  "dependencies": {
    "opencollective-postinstall": "^2.0.2",
    "opencollective": "^1.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/react-router"
  }
}